{
  "description": "Arcad App for Merlin labs.",
  "version": "0.0.1",
  "includePath": [
    "SAMCO_SRC/QPROTOSRC"
  ],
  "repository": "git@github.com:NicolaeCh/ibmi-samco.git",
  "objlib": "&verlibsamco",
  "curlib": "&CURLIB",
  "extensions": {
    "arcad": {
      "code": "SAMCO",
      "libraryPrefix": "SAM",
      "iasp": "*SYSBAS",
      "validated": true
    }
  },
  "buildCommand": "elias compile {branch}",
  "compileCommand": "elias compile {branch} -f {filename}"
}